          <metadata></metadata>
          <section id="3467-title">
            <fragment id="3467-1">
              <heading prefix="6" level="1" numbered="true" id="3467-1-1-1">Firmware Specification</heading>
            </fragment>
          </section>
          <section id="3467-content">
            <fragment id="3467-2">
              <heading prefix="6.1" level="2" numbered="true" id="3467-1-2-1">Logic flow</heading>
              <nlist start="1">
                <item>Boot system.</item>
                <item>Read switch state.</item>
                <item>If switch = OFF → idle.</item>
                <item>If switch = ON:
                  <list>
                    <item>Delay 300–600 ms (optional dramatic pause).</item>
                    <item>Move servo to extension angle.</item>
                    <item>Wait 300 ms.</item>
                    <item>Move servo to retraction angle.</item>
                    <item>Return to idle.</item>
                  </list>
                </item>
              </nlist>
            </fragment>
            <fragment id="3467-3">
              <heading prefix="6.2" level="2" numbered="true" id="3467-1-3-1">Pseudo-code</heading>
              <preformat>loop:
  if switch == ON:
    delay(400)
    servo.write(EXTEND)
    delay(300)
    servo.write(RETRACT)
  else:
    idle()
              </preformat>
            </fragment>
          </section>
        </document>
      </blockxref>
